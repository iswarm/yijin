.logo{
    height: 200rpx;
	width: 200rpx;
	padding-top: 50rpx;
	padding-left: 35%;
}
.login{
	width:100%;
	height:100%;
	background-size: contain;
	position: flex;/**设置定位类型*/
	z-index:3000 ;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;/**flex布局*/
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	    -ms-flex-direction: column;
	        flex-direction: column;/**flex布局方向*/
	margin-top: 80rpx;
}
.login-xy{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: horizontal;
	-webkit-box-direction: normal;
	-webkit-flex-direction: row;
	    -ms-flex-direction: row;
	        flex-direction: row;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;/**flex布局控件位置居中*/
	margin-bottom: 20rpx;
	margin-top: 20rpx;
}
.login-txt{
	color: #333333;
	font-size: 35rpx;
}
.line{
	width:70%;
	height:1px;
	border-top:solid black 1px;
	margin-left: 15%;
}
.forgot-psd{
	color: black;
	width: 80%;
	text-align: right;
	margin-bottom: 30rpx;
}
.button{
	width: 60%;
}

